{
  "name": "@apollosproject/ui-content",
  "version": "0.8.6",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "src/index.js",
  "files": [
    "src/**",
    "storybook/**"
  ],
  "scripts": {
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "test": "jest",
    "lint": "eslint ./src --ext .js"
  },
  "keywords": [
    "react-native-component",
    "react-component",
    "react-native"
  ],
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|rn-*|react-clone-referenced-element|react-navigation|@apollosproject))"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(test|tests).{js,jsx,mjs}"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "transform": {
      ".+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "peerDependencies": {
    "@apollosproject/ui-kit": "*",
    "@apollosproject/ui-storybook": "*",
    "react": "*",
    "react-native": "*",
    "react-navigation": "*"
  },
  "dependencies": {
    "@apollosproject/ui-kit": "^0.8.6",
    "react-apollo": "2.5.5",
    "react-navigation": "2.18.3"
  },
  "devDependencies": {
    "jest": "24.8.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.1",
    "react": "^16.8.3",
    "react-native": "0.59.8",
    "react-test-renderer": "16.6.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}
