{
  "name": "@apollosproject/test-utils",
  "version": "1.1.0-beta.0",
  "homepage": "https://github.com/ApollosProject/apollos-prototype#readme",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production rm -rf ./lib && mkdir -p lib && babel ./src -d ./lib",
    "lint": "eslint ./src --ext .js"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test|tests).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "reporters": [
      "<rootDir>/../../scripts/fingers-crossed-reporter.js"
    ]
  },
  "peerDependencies": {
    "@apollosproject/ui-kit": "*"
  },
  "devDependencies": {
    "@apollosproject/babel-preset-apollos": "^1.1.0-beta.0"
  },
  "dependencies": {
    "@apollosproject/ui-kit": "^1.1.0-beta.0",
    "prop-types": "15.6.1",
    "react": "^16.8.3",
    "react-apollo": "2.5.5",
    "react-test-renderer": "16.6.1",
    "waait": "^1.0.4"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "eb803583da83beeab7756baddcae3adca7650c39"
}
